{
  "functions": {
    "src/app/api/scan/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "src/app/api/scan/[id]/route.ts": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate" }
      ]
    }
  ]
}
